</div>

<div class="table-container">
<table class="table" id="tabla" style="width: 90%;font-family: revert;font-size: 25px;">
    <tr style="background-color: lightblue;color: #5271FF;height: 85px;">
        <th>Usuario</th>
        <th>Puntaje</th>
    </tr>

    {{#each usuarios}}
    <tr>
        <td>{{this.usuario}}</td>
        <td>{{this.puntuacion_max}}</td>
    </tr>
    {{/each}}
    {{#each usuarios}}
    <select>
        <option>{{this.usuario}}</option>
    </select>
    {{/each}}
</table>
</div>

